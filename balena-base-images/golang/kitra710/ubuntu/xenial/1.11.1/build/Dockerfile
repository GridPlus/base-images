	&& echo "3e15fb6706b2ab2bd76c18cab790b1e9c1afd90abad27f58cafabe571a702244  go$GO_VERSION.linux-aarch64.tar.gz" | sha256sum -c - \
	&& tar -xzf "go$GO_VERSION.linux-aarch64.tar.gz" -C /usr/local/go --strip-components=1 \
	&& rm -f go$GO_VERSION.linux-aarch64.tar.gz

